alias eject='sudo diskutil unmount'
alias vi='vim'
alias v='vim'
alias ll='ls -alt'
alias l='ls -at'
alias mplayer='mplayer -subdelay 100000 -fs -geometry 50%:50%'
alias music='mplayer -lavdopts threads=2 -loop 0 -shuffle -geometry 50%:50%'
alias c='clear'
alias ga='git add'
alias gm='git commit -m'
alias am='git commit -am'
alias gd='git diff'
alias gp='git push'
alias ungzip='gzip -d'
alias untar='tar xvf'
alias d='download'
alias time='exe time'
