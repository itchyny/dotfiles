# copied from .zshrc
# note that edit .zshrc first
alias cdup='cd ../'
alias gosh='rlwrap gosh'
alias coqtop='rlwrap coqtop'
alias vi='vim'
alias hs='cd ~/Dropbox/hs/'
alias js='cd ~/Dropbox/js/'
alias univ='cd ~/Dropbox/univ/'
alias port='/usr/bin/env port'
alias v='vim'
alias mpg123='mpg123 -zC'
alias asm=runcpp
alias mp3='mplayer'
alias rm='rm -i'
alias cp='cp -i'
alias mv='mv -i'
alias emacs='vi'
alias yoruho='python ~/Dropbox/py/itchyny/yoruho.py'
alias tweet='python ~/Dropbox/py/itchyny/tweet.py'
alias twitter='runhaskell ~/Dropbox/hs/twitter/twitter.hs'
alias ll='ls -alt'
alias port_update='sudo port selfupdate && sudo port -d sync  && sudo port outdated && sudo port upgrade outdated'
alias mplayer='mplayer -lavdopts threads=2 -loop 0 -shuffle'
alias c='clear'
alias wget="wget --user-agent='Mozilla/5.0 (Macintosh; Intel Mac OS X 10_8_2) AppleWebKit/537.4 (KHTML, like Gecko) Chrome/22.0.1229.94 Safari/537.4' "
alias am='git commit -am'
alias gd='git diff'
alias ungzip='gzip -d'
